{
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --extensions '.ts,.tsx'",
    "lint": "yarn tslint --project . && eslint src",
    "clean": "rimraf lib",
    "prepublish": "yarn run clean && yarn run build",
    "start": "babel-node --extensions '.ts,.tsx' src/index.ts",
    "postinstall": "ls lib 2> /dev/null > /dev/null || (yarn install --ignore-scripts && yarn prepublish)"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^1.6.7",
    "dataloader": "^1.3.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/node": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-3": "^7.0.0-beta.38",
    "@babel/preset-typescript": "^7.0.0-beta.38",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.5.0",
    "typescript": "^2.1.4"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
