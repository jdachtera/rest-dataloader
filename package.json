{
  "name": "rest-dataloader",
  "version": "1.0.3",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "yarn rollup -c -w",
    "lint": "yarn tslint --project . && eslint src",
    "clean": "rimraf lib",
    "prepare": "yarn run clean && yarn run build",
    "start": "ts-node src/index.ts",
    "postinstall": "ls lib 2> /dev/null > /dev/null || (yarn install --ignore-scripts && yarn prepare)"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash.merge": "^4.6.3",
    "@types/node": "^9.4.2",
    "@types/node-fetch": "^1.6.7",
    "dataloader": "^1.4.0",
    "lodash": "^4.17.4",
    "universal-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.1.4",
    "jest-fetch-mock": "^1.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.3",
    "rollup-plugin-closure-compiler": "^0.1.0",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-typescript2": "^0.11.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "ts-jest": "^22.0.3",
    "tsickle": "^0.26.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.5.0",
    "typescript": "^2.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts}"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "clover",
      "text",
      "text-summary",
      "lcov"
    ],
    "testMatch": [
      "**/src/?(*.)(spec|test).js?(x)",
      "**/src/?(*.)(spec|test).ts?(x)"
    ]
  }
}
